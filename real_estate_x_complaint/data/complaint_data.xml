<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- SET DEFAULT STAGES -->
    <record id="complaint_complaint_stage_0" model="complaint.stage">
        <field name="sequence">10</field>
        <field name="name">New</field>
    </record>

    <record id="complaint_complaint_stage_1" model="complaint.stage">
        <field name="sequence">20</field>
        <field name="name">In Review</field>
    </record>

    <record id="complaint_complaint_stage_2" model="complaint.stage">
        <field name="sequence">30</field>
        <field name="name">In Progress</field>
    </record>

    <record id="complaint_complaint_stage_3" model="complaint.stage">
        <field name="sequence">40</field>
        <field name="name">Solved</field>
        <field name="fold" eval="True"/>
    </record>

    <record id="complaint_complaint_stage_4" model="complaint.stage">
        <field name="sequence">50</field>
        <field name="name">Dropped</field>
        <field name="fold" eval="True"/>
    </record>
</odoo>